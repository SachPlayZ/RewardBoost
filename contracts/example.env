# Example environment configuration for Quest Rewards deployment
# Copy this to .env and fill in your actual values

# Deployer wallet private key (without 0x prefix)
PRIVATE_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Treasury address to receive platform fees
TREASURY_ADDRESS=0x742d35Cc6665C6E4E4f05f1c9F11b9c0c8e19b59

# Network RPC URLs
ETHEREUM_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key
POLYGON_RPC_URL=https://polygon-mainnet.alchemyapi.io/v2/your-api-key
MUMBAI_RPC_URL=https://polygon-mumbai.g.alchemy.com/v2/your-api-key
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your-api-key
SEI_RPC_URL=https://evm-rpc.sei-apis.com

# Custom token addresses (for networks not pre-configured)
USDC_ADDRESS=0xA0B86a33e6e5e9EB02b6816E4D8F5C9A6c8A9A1B
SEI_ADDRESS=0x0000000000000000000000000000000000000000

# Block explorer API keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
